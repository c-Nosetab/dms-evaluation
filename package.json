{
  "name": "dms-evaluation",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:web": "turbo run build --filter=@dms/web",
    "build:api": "turbo run build --filter=@dms/api",
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=@dms/web",
    "dev:api": "turbo run dev --filter=@dms/api",
    "start:web": "cd apps/web && npm run start",
    "start:api": "cd apps/api && npm run db:push && npm run start:prod",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.7.2",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@radix-ui/react-context-menu": "^2.2.16"
  }
}
